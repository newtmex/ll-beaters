{
  "name": "@ll-beaters/nextjs",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "start": "next dev",
    "build": "next build",
    "serve": "next start",
    "format": "prettier --write . '!(node_modules|.next|contracts)/**/*'",
    "check-types": "tsc --noEmit --incremental",
    "vercel": "vercel",
    "vercel:yolo": "vercel --build-env NEXT_PUBLIC_IGNORE_BUILD_ERROR=true",
    "clean": "rimraf .next",
    "predev": "yarn graphql-codegen",
    "prebuild": "yarn graphql-codegen",
    "lint": "next lint && yarn lint:prettier",
    "lint:prettier": "prettier \"**/*.{js,ts,tsx,json,md,gql,graphql}\" --check",
    "analyze": "ANALYZE=true yarn build",
    "graphql-codegen": "graphql-codegen --overwrite"
  },
  "dependencies": {
