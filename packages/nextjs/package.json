    "@uniswap/v2-sdk": "^3.0.1",
    "blo": "^1.0.1",
    "daisyui": "^4.6.0",
    "ethers": "5.7.2",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
